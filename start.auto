#!/usr/bin/perl -w
use strict;

my ($log_name) = @ARGV;
if ((not defined $log_name)) {
    print "\nUsage: $0 <log.name.ext>\n";
    exit 0;
}

my $cmd = "python dispatcher.py >& dlog$log_name &";
      print "$cmd\n";
      system($cmd);
      if ( $? ) { die "Command failed: $cmd: $!"; }

$cmd = "python manager.py >& mlog$log_name &";
      print "$cmd\n";
     system($cmd);
      if ( $? ) { die "Command failed: $cmd: $!"; }

